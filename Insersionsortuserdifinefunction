#include <stdio.h>

// User-defined function for insertion sort
void insertionSort(int arr[], int n) {
    int i, j, key;
    for (i = 1; i < n; i++) {
        key = arr[i];
        j = i - 1;

        // Move elements greater than key to one position ahead
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j = j - 1;
        }
        arr[j + 1] = key;
    }
}

int main() {
    int arr[] = {50, 20, 40, 10, 30}; // array elements
    int n = 5;
    int i;

    printf("Before sorting:\n");
    for (i = 0; i < n; i++)
        printf("%d ", arr[i]);

    // Function call
    insertionSort(arr, n);

    printf("\n\nAfter sorting:\n");
    for (i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
